      "source": [
        "inp = int(input())\n",
        "le = len(str(inp))\n",
        "string = \"\"\n",
        "for x in range(le):\n",
        "  if le-1:\n",
        "    string += str(inp // (10 ** (le-1))) + \", \"\n",
        "    inp %= (10 ** (le-1))\n",
        "    le -= 1\n",
        "  else:\n",
        "    string += str(inp // (10 ** (le-1)))\n",
        "    inp %= (10 ** (le-1))\n",
        "print(string)"
      ]
