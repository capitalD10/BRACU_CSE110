        "inp = int(input())\n",
        "le = len(str(inp))\n",
        "string = \"\"\n",
        "count = 0\n",
        "for i in range(le):\n",
        "  if count == le-1:\n",
        "    string += str(inp % 10)\n",
        "    inp //=10\n",
        "    count += 1\n",
        "  else:\n",
        "    string += str(inp % 10) + \", \"\n",
        "    inp //= 10\n",
        "    count += 1\n",
        "print(string)"
      ]
