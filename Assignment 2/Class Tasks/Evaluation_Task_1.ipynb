        "inp1 = int(input(\"Hours worked: \"))\n",
        "\n",
        "if inp1 < 0:\n",
        "  print(\"Hour cannot be negative\")\n",
        "elif inp1 > 168:\n",
        "  print(\"Impossible to work more than 168 hours weekly\")\n",
        "elif inp1 <= 40:\n",
        "  inp1 *= 200\n",
        "  print(inp1)\n",
        "elif 40 <= inp1 <= 168:\n",
        "  inp1 =(inp1 - 40)*300 + 8000\n",
        "  print(inp1)"
