      "source": [
        "inp1 = int(input(\"Given Seconds: \"))\n",
        "\n",
        "hours = inp1 // 3600\n",
        "\n",
        "remain_sec = inp1 % 3600\n",
        "minutes = remain_sec // 60\n",
        "\n",
        "seconds = remain_sec % 60\n",
        "\n",
        "print(\"Hours:\", hours)\n",
        "print(\"Minutes:\", minutes)\n",
        "print(\"Seconds:\", seconds)"
      ]
