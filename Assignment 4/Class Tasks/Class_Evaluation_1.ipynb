      "source": [
        "inp = input()\n",
        "bin_count = 0\n",
        "non_count = 0\n",
        "for x in range(len(inp)):\n",
        "  if inp[x] == \"1\" or inp[x] == \"0\":\n",
        "    bin_count += 1\n",
        "  else:\n",
        "    non_count += 1\n",
        "if non_count > 0:\n",
        "  print(\"Not a Binary Number\")\n",
        "else:\n",
        "  print(\"Binary Number\")"
      ]
    }
  ]
